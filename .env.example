TSLA_PERCENTAGE=0.2222
NVDA_PERCENTAGE=0.2222
QQQ_PERCENTAGE=0.2222
BTB_PERCENTAGE=0.3333

# Configure your bot session TTL
TIMEOUT=4500
WARNING_TIME=3000

LOGS_PATH=./logs